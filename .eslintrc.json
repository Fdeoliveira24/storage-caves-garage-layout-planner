{
  "env": { "es2022": true, "browser": true, "node": true },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
  },
  "globals": {
    "fabric": "readonly",
    "turf": "readonly",
    "jsPDF": "readonly",
    "MobileUIManager": "readonly"
  }
}
